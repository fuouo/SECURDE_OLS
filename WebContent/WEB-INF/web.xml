<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:c="http://java.sun.com/jsp/jstl/core" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>securde_ols</display-name>
  <welcome-file-list>
    <welcome-file>StartServlet</welcome-file>
  </welcome-file-list>
  
  <session-config>
    <session-timeout>15</session-timeout>
    <tracking-mode>COOKIE</tracking-mode>
  </session-config>
  
  <error-page>
	<location>/WEB-INF/error/error-default.jsp</location>
  </error-page>
  
</web-app>